<?xml version="1.0" encoding="UTF-8" ?>
<items xmlns="https://connectorio.com/xmlns/managed/items/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="
    https://connectorio.com/xmlns/managed/items/1.0.0
    https://connectorio.com/xmlns/managed/items-1.0.0.xsd
  ">

  <item name="Foo" label="Custom item" category="Test">
    <type>Number:Energy</type>
    <tag>Measurement</tag>
    <tag>Energy</tag>
    <group>A</group>
    <group>B</group>
    <metadata>
      <state-description>
        <pattern>%d</pattern>
      </state-description>
      <co7io-test value="yes">
        <name>x</name>
        <multiple class="list">
          <string>1</string>
          <string>2</string>
        </multiple>
      </co7io-test>
      <co7io-abc>
        <label>y</label>
      </co7io-abc>
    </metadata>
    <link>
      <channel>foo:bar:baz</channel>
      <config>
        <offset>1</offset>
        <profile>system:offset</profile>
      </config>
    </link>
  </item>

  <group name="y">
    <type>Group</type>
    <member>Foo</member>
  </group>

</items>